      <Query>
        <DataSourceName>ETCMDataSource</DataSourceName>
        <CommandText>SELECT Project FROM ActiveProjects</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
